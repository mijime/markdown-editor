{
  "dependencies": {
    "bytesize-icons": "^1.1.0",
    "milligram": "^1.3.0",
    "preact": "^7.2.1",
    "preact-compat": "^3.14.3",
    "prismjs": "^1.6.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-milligram": "file:packages/react-milligram",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "rehype-prism": "file:packages/rehype-prism",
    "rehype-react": "^3.0.0",
    "remark": "^7.0.0",
    "remark-emoji": "^2.0.0",
    "remark-rehype": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-babili": "^0.0.12",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.27.3",
    "esdoc": "^0.5.2",
    "eslint": "^3.18.0",
    "eslint-config-eslint": "^4.0.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "flow-bin": "^0.43.0",
    "flow-jsdoc": "^0.3.0",
    "gh-pages": "^0.12.0",
    "html-webpack-inline-source-plugin": "^0.0.7",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "node-sass": "^4.5.1",
    "react-test-renderer": "^15.4.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.14.1",
    "svg-react-loader": "^0.3.7",
    "uglify-js": "https://github.com/mishoo/UglifyJS2.git#harmony",
    "uglifyjs-webpack-plugin": "^0.4.0",
    "webpack": "^2.2.1",
    "webpack-bundle-size-analyzer": "^2.5.0",
    "webpack-dev-server": "^2.4.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "babili"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(svg)$": "<rootDir>/src/__mocks__/component.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/mock.js",
      "\\.(css|less|sass)$": "<rootDir>/src/__mocks__/mock.js"
    }
  },
  "scripts": {
    "fix": "eslint --fix .",
    "test": "yarn run -- fix && jest --coverage",
    "build": "yarn run -- test && webpack --config webpackfile/prod.js",
    "deploy": "yarn run -- build && gh-pages -d build/Release",
    "analyzer": "yarn run -- test && webpack --json | webpack-bundle-size-analyzer",
    "debug": "yarn run -- test && webpack-dev-server",
    "flow": "yarn run -- fix && flow",
    "doc": "esdoc",
    "gen-flow-files": "mkdir -p flow-files && flow gen-flow-files src --out-dir flow-files"
  },
  "name": "markdown-editor",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT"
}
