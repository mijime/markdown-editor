{
  "dependencies": {
    "bytesize-icons": "^1.1.0",
    "milligram": "^1.3.0",
    "node-emoji": "^1.5.1",
    "preact": "^8.2.6",
    "preact-compat": "^3.17.0",
    "preact-redux": "^2.0.3",
    "prismjs": "^1.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-milligram": "file:./packages/react-milligram",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "rehype-prism": "file:./packages/rehype-prism",
    "rehype-react": "^3.0.1",
    "remark": "^7.0.1",
    "remark-rehype": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.18.0",
    "eslint-config-eslint": "^4.0.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "gh-pages": "^1.0.0",
    "html-webpack-inline-source-plugin": "^0.0.8",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "prettier": "^1.8.2",
    "react-test-renderer": "^16.1.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "svg-react-loader": "^0.4.5",
    "uglify-js": "^3.0.7",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^3.8.1",
    "webpack-bundle-size-analyzer": "^2.5.0",
    "webpack-dev-server": "^2.4.5"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "minify"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/build/annotated/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/build/"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/build/"
    ],
    "moduleNameMapper": {
      "\\.(svg)$": "<rootDir>/src/__mocks__/component.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/mock.js",
      "\\.(css|less|sass)$": "<rootDir>/src/__mocks__/mock.js"
    }
  },
  "scripts": {
    "lint": "prettier --tab-width 4 --write '*.js' 'webpackfile/**/*.js' 'src/**/*.js' 'packages/**/*.js' && eslint --fix .",
    "test": "npm run lint && jest --coverage src",
    "build": "npm run lint && webpack --config webpackfile/prod.js",
    "deploy": "npm run build && gh-pages -d build/Release",
    "analyzer": "npm run test && webpack --json | webpack-bundle-size-analyzer",
    "debug": "npm run test && webpack-dev-server --config webpackfile/debug.js"
  },
  "name": "markdown-editor",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT"
}
