{
  "dependencies": {
    "bytesize-icons": "^1.1.0",
    "milligram": "^1.3.0",
    "node-emoji": "^1.5.1",
    "preact": "^8.1.0",
    "preact-compat": "^3.16.0",
    "prismjs": "^1.6.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-milligram": "file:packages/react-milligram",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "rehype-prism": "file:packages/rehype-prism",
    "rehype-react": "^3.0.0",
    "remark": "^7.0.1",
    "remark-rehype": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^7.0.0",
    "babel-preset-babili": "^0.0.12",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.28.1",
    "esdoc": "^0.5.2",
    "eslint": "^3.18.0",
    "eslint-config-eslint": "^4.0.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-jest": "^20.0.1",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "flow-bin": "^0.46.0",
    "flow-jsdoc": "^0.3.0",
    "gh-pages": "^1.0.0",
    "html-webpack-inline-source-plugin": "^0.0.8",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.1",
    "node-sass": "^4.5.3",
    "react-styleguidist": "^5.2.1",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "svg-react-loader": "^0.3.7",
    "uglify-js": "https://github.com/mishoo/UglifyJS2.git#harmony",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^2.5.1",
    "webpack-bundle-size-analyzer": "^2.5.0",
    "webpack-dev-server": "^2.4.5"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "babili"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/build/annotated/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/build/"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/build/"
    ],
    "moduleNameMapper": {
      "\\.(svg)$": "<rootDir>/src/__mocks__/component.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/mock.js",
      "\\.(css|less|sass)$": "<rootDir>/src/__mocks__/mock.js"
    }
  },
  "scripts": {
    "lint": "eslint --fix .",
    "check": "yarn run -- lint && flow-jsdoc -d src -o build/annotated && flow",
    "test": "yarn run -- check && jest --coverage src",
    "build": "yarn run -- test && esdoc && webpack --config webpackfile/prod.js",
    "deploy": "yarn run -- build && gh-pages -d build/Release",
    "analyzer": "yarn run -- test && webpack --json | webpack-bundle-size-analyzer",
    "doc": "yarn run -- lint && esdoc && styleguidist build",
    "debug": "yarn run -- test && webpack-dev-server --config webpackfile/debug.js"
  },
  "name": "markdown-editor",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT"
}
